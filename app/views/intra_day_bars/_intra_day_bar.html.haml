%tr[intra_day_bar]{:class => cycle('list-line-even', 'list-line-odd')}
  %td.ticker_id
    = lkup intra_day_bar.ticker
  %td.interval
    = h intra_day_bar.interval
  %td.start_time
    = h intra_day_bar.start_time
  %td.open
    = h intra_day_bar.open
  %td.close
    = h intra_day_bar.close
  %td.high
    = h intra_day_bar.high
  %td.volume
    = h intra_day_bar.volume
  %td.delta
    = h intra_day_bar.delta
  %td.show
    = link_to 'Show', object_path(intra_day_bar)
  %td.edit
    = link_to 'Edit', edit_object_path(intra_day_bar)
  %td.destroy
    = link_to 'Destroy', object_path(intra_day_bar), :confirm => 'Really destroy intra_day_bar?', :method => :delete
