%tr[order]{:class => cycle('list-line-even', 'list-line-odd')}
  %td.txn
    = h order.txn
  %td.type
    = h order.type
  %td.expiration
    = h order.expiration
  %td.quantity
    = h order.quantity
  %td.placed_at
    = h order.placed_at
  %td.filled_at
    = h order.filled_at
  %td.activation_price
    = h order.activation_price
  %td.order_price
    = h order.order_price
  %td.fill_price
    = h order.fill_price
  %td.ticker_id
    = lkup order.ticker
  %td.position_id
    = lkup order.position
  %td.show
    = link_to 'Show', object_path(order)
  %td.edit
    = link_to 'Edit', edit_object_path(order)
  %td.destroy
    = link_to 'Destroy', object_path(order), :confirm => 'Really destroy order?', :method => :delete
