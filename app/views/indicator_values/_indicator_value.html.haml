%tr[indicator_value]{:class => cycle('list-line-even', 'list-line-odd')}
  %td.indicator_id
    = lkup indicator_value.indicator
  %td.datetime
    = h indicator_value.datetime
  %td.value
    = h indicator_value.value
  %td.show
    = link_to 'Show', object_path(indicator_value)
  %td.edit
    = link_to 'Edit', edit_object_path(indicator_value)
  %td.destroy
    = link_to 'Destroy', object_path(indicator_value), :confirm => 'Really destroy indicator_value?', :method => :delete
