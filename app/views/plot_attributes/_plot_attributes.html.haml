%tr[plot_attributes]{:class => cycle('list-line-even', 'list-line-odd')}
  %td.name
    = h plot_attributes.name
  %td.symbol_id
    = lkup plot_attributes.symbol
  %td.type
    = h plot_attributes.type
  %td.anchor_date
    = h plot_attributes.anchor_date
  %td.period
    = h plot_attributes.period
  %td.attributes
    = h plot_attributes.attributes
  %td.show
    = link_to 'Show', object_path(plot_attributes)
  %td.edit
    = link_to 'Edit', edit_object_path(plot_attributes)
  %td.destroy
    = link_to 'Destroy', object_path(plot_attributes), :confirm => 'Really destroy plot_attributes?', :method => :delete
