%tr[industry]{:class => cycle('list-line-even', 'list-line-odd')}
  %td.name
    = h industry.name
  %td.show
    = link_to 'Show', object_path(industry)
  %td.edit
    = link_to 'Edit', edit_object_path(industry)
  %td.destroy
    = link_to 'Destroy', object_path(industry), :confirm => 'Really destroy industry?', :method => :delete
