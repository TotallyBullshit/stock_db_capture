%tr[sim_summary]{:class => cycle('list-line-even', 'list-line-odd')}
  %td.sim_date
    = h sim_summary.sim_date
  %td.positions_held
    = h sim_summary.positions_held
  %td.positions_available
    = h sim_summary.positions_available
  %td.portfolio_value
    = h sim_summary.portfolio_value
  %td.cash_balance
    = h sim_summary.cash_balance
  %td.show
    = link_to 'Show', object_path(sim_summary)
  %td.edit
    = link_to 'Edit', edit_object_path(sim_summary)
  %td.destroy
    = link_to 'Destroy', object_path(sim_summary), :confirm => 'Really destroy sim_summary?', :method => :delete
