%tr[daily_return]{:class => cycle('list-line-even', 'list-line-odd')}
  %td.volume
    = h daily_return.volume
  %td.ask
    = h daily_return.ask
  %td.bid
    = h daily_return.bid
  %td.day_range_low
    = h daily_return.day_range_low
  %td.day_range_high
    = h daily_return.day_range_high
  %td.change_percent
    = h daily_return.change_percent
  %td.last_trade_date
    = h daily_return.last_trade_date
  %td.tickertrend
    = h daily_return.tickertrend
  %td.change_points
    = h daily_return.change_points
  %td.open
    = h daily_return.open
  %td.previous_close
    = h daily_return.previous_close
  %td.last_trade
    = h daily_return.last_trade
  %td.avg_volumn
    = h daily_return.avg_volumn
  %td.day_low
    = h daily_return.day_low
  %td.last_trade_time
    = h daily_return.last_trade_time
  %td.day_high
    = h daily_return.day_high
  %td.ticker_id
    = lkup daily_return.ticker
  %td.created_at
    = h daily_return.created_at
  %td.updated_at
    = h daily_return.updated_at
  %td.show
    = link_to 'Show', object_path(daily_return)
  %td.edit
    = link_to 'Edit', edit_object_path(daily_return)
  %td.destroy
    = link_to 'Destroy', object_path(daily_return), :confirm => 'Really destroy daily_return?', :method => :delete
