%tr[scan]{:class => cycle('list-line-even', 'list-line-odd')}
  %td.name
    = h scan.name
  %td.start
    = h scan.start_date
  %td.end
    = h scan.end_date
  %td.sql_query
    = h scan.conditions
  %td.show
    = link_to 'Show', object_path(scan)
  %td.edit
    = link_to 'Edit', edit_object_path(scan)
  %td.destroy
    = link_to 'Destroy', object_path(scan), :confirm => 'Really destroy scan?', :method => :delete
