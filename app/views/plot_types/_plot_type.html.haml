%tr[plot_type]{:class => cycle('list-line-even', 'list-line-odd')}
  %td.name
    = h plot_type.name
  %td.source_model
    = h plot_type.source_model
  %td.method
    = h plot_type.method
  %td.time_class
    = h plot_type.time_class
  %td.resolution
    = h plot_type.resolution
  %td.inputs
    = h plot_type.inputs
  %td.num_outputs
    = h plot_type.num_outputs
  %td.show
    = link_to 'Show', object_path(plot_type)
  %td.edit
    = link_to 'Edit', edit_object_path(plot_type)
  %td.destroy
    = link_to 'Destroy', object_path(plot_type), :confirm => 'Really destroy plot_type?', :method => :delete
