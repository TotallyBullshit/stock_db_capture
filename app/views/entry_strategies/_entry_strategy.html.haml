%tr[entry_strategy]{:class => cycle('list-line-even', 'list-line-odd')}
  %td.name
    = h entry_strategy.name
  %td.params
    = h entry_strategy.params
  %td.description
    = h entry_strategy.description
  %td.show
    = link_to 'Show', object_path(entry_strategy)
  %td.edit
    = link_to 'Edit', edit_object_path(entry_strategy)
  %td.destroy
    = link_to 'Destroy', object_path(entry_strategy), :confirm => 'Really destroy entry_strategy?', :method => :delete
