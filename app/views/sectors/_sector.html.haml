%tr[sector]{:class => cycle('list-line-even', 'list-line-odd')}
  %td.name
    = h sector.name
  %td.show
    = link_to 'Show', object_path(sector)
  %td.edit
    = link_to 'Edit', edit_object_path(sector)
  %td.destroy
    = link_to 'Destroy', object_path(sector), :confirm => 'Really destroy sector?', :method => :delete
