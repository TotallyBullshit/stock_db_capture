%tr[tda_position]{:class => cycle('list-line-even', 'list-line-odd')}
  %td.ticker_id
    = lkup tda_position.ticker
  %td.estrategy_id
    = lkup tda_position.estrategy
  %td.xstrategy_id
    = lkup tda_position.xstrategy
  %td.entry_date
    = h tda_position.entry_date
  %td.exit_date
    = h tda_position.exit_date
  %td.openned_at
    = h tda_position.openned_at
  %td.closed_at
    = h tda_position.closed_at
  %td.updated_at
    = h tda_position.updated_at
  %td.entry_price
    = h tda_position.entry_price
  %td.exit_price
    = h tda_position.exit_price
  %td.curr_price
    = h tda_position.curr_price
  %td.eorderid
    = h tda_position.eorderid
  %td.xorderid
    = h tda_position.xorderid
  %td.rum_shares
    = h tda_position.rum_shares
  %td.stop_loss
    = h tda_position.stop_loss
  %td.days_held
    = h tda_position.days_held
  %td.nreturn
    = h tda_position.nreturn
  %td.rretrun
    = h tda_position.rretrun
  %td.show
    = link_to 'Show', object_path(tda_position)
  %td.edit
    = link_to 'Edit', edit_object_path(tda_position)
  %td.destroy
    = link_to 'Destroy', object_path(tda_position), :confirm => 'Really destroy tda_position?', :method => :delete
