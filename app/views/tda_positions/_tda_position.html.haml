%tr[tda_position]{:class => cycle('list-line-even', 'list-line-odd')}
  %td.ticker_id
    = lkup tda_position.ticker, :symbol
  %td.center
    = h tda_position.opened_at.to_formatted_s(:twz)
  %td.center
    = h tda_position.closed_at ? tda_position.closed_at.to_formatted_s(:twz) : '-'
  %td.center
    = h tda_position.entry_price
  %td.center
    = h tda_position.exit_price
  %td.center
    = h tda_position.curr_price
  %td.center
    = h tda_position.num_shares
  %td.center
    = h tda_position.days_held
  %td.center
    = h tda_position.nreturn
  %td.center
    = h tda_position.rretrun
  %td.show
    = link_to 'Show', object_path(tda_position)
  %td.edit
    = link_to 'Edit', edit_object_path(tda_position)
  %td.destroy
    = link_to 'Destroy', object_path(tda_position), :confirm => 'Really destroy tda_position?', :method => :delete
