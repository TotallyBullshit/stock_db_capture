%tr[indicator]{:class => cycle('list-line-even', 'list-line-odd')}
  %td.name
    = h indicator.name
  %td.show
    = link_to 'Show', object_path(indicator)
  %td.edit
    = link_to 'Edit', edit_object_path(indicator)
  %td.destroy
    = link_to 'Destroy', object_path(indicator), :confirm => 'Really destroy indicator?', :method => :delete
