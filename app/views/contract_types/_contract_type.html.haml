%tr[contract_type]{:class => cycle('list-line-even', 'list-line-odd')}
  %td.name
    = h contract_type.name
  %td.show
    = link_to 'Show', object_path(contract_type)
  %td.edit
    = link_to 'Edit', edit_object_path(contract_type)
  %td.destroy
    = link_to 'Destroy', object_path(contract_type), :confirm => 'Really destroy contract_type?', :method => :delete
