%tr[study]{:class => cycle('list-line-even', 'list-line-odd')}
  %td.name
    = h study.name
  %td.start_date
    = h study.start_date
  %td.end_date
    = h study.end_date
  %td.show
    = link_to 'Show', object_path(study)
  %td.edit
    = link_to 'Edit', edit_object_path(study)
  %td.destroy
    = link_to 'Destroy', object_path(study), :confirm => 'Really destroy study?', :method => :delete
