%tr[historical_attribute]{:class => cycle('list-line-even', 'list-line-odd')}
  %td.name
    = h historical_attribute.name
  %td.show
    = link_to 'Show', object_path(historical_attribute)
  %td.edit
    = link_to 'Edit', edit_object_path(historical_attribute)
  %td.destroy
    = link_to 'Destroy', object_path(historical_attribute), :confirm => 'Really destroy historical_attribute?', :method => :delete
