%tr[position_series]{:class => cycle('list-line-even', 'list-line-odd')}
  %td.position_id
    = lkup position_series.position
  %td.date
    = h position_series.date
  %td.indicator_id
    = lkup position_series.indicator
  %td.value
    = h position_series.value
  %td.show
    = link_to 'Show', object_path(position_series)
  %td.edit
    = link_to 'Edit', edit_object_path(position_series)
  %td.destroy
    = link_to 'Destroy', object_path(position_series), :confirm => 'Really destroy position_series?', :method => :delete
